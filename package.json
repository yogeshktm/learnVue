{
  "version": "1.0.0",
  "name": "wsf-demo",
  "description": "wsf-demo",
  "private": true,
  "main": "gulpfile.js",
  "scripts": {
    "doc": "node node_modules/jsdoc/jsdoc.js --configure ./Tasks/jsdoc.json --debug",
    "build": "gulp build --env production",
    "test": "jest --config jest.config.json --coverage --no-cache",
    "build:develop": "gulp build",
    "build:js": "gulp scripts",
    "develop": "gulp develop",
    "lint": "gulp eslint",
    "lint:styles": "stylelint \"src/{Feature,Foundation,Project}/**/*.scss\" --fix",
    "lint:styles:css": "stylelint \"src/{Feature,Foundation,Project}/**/*.css\" --fix",
    "css:test": "npm run lint:styles && lint:styles:css",
    "precommit": "lint-staged && npm run test"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": ".",
    "outputName": "./coverage/junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "browser": {
